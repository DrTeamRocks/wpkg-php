{
    "name": "wpkg/wpkg-php",
    "description": "WPKG config generator on PHP",
    "homepage": "https://wpkg.org/",
    "keywords": [
        "wpkg",
        "xml"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "homepage": "http://drteam.rocks/",
            "email": "paul@drteam.rocks",
            "name": "Paul Rock",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "WPKG\\": "src/"
        }
    },
    "repositories": {
        "wpkg/wpkg-js": {
            "type": "git",
            "url": "https://github.com/wpkg/wpkg-js"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-xml": "*",
        "wpkg/wpkg-js": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "adldap/adldap": "@dev"
    },
    "minimum-stability": "stable"
}
